
pipeline{

agent any
stages{
stage ('backup')
{
steps{
  sh '''
     cd /var/lib
     zip -r ${WORKSPACE}/backup.zip jenkins	 
  '''

}


}
stage ('upload file into s3 bucket')
{
steps{
  script{
  s3Upload consoleLogLevel: 'INFO', dontSetBuildResultOnFailure: false, dontWaitForConcurrentBuildCompletion: false, pluginFailureResultConstraint: 'FAILURE', profileName: 'jenkins-backup', userMetadata: []
  }

}

}

}
}
